/*******************************
            Folders
*******************************/

/* Path to theme packages */
@themesFolder : 'themes/';

/* Path to site override folder */
@siteFolder   : 'site/';

@deviceFolder : 'desktop';


/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below

   Be sure to update the user folder name (see README)
*/

/* Global */
@site       : 'tricae';
@reset      : 'tricae';

/* Elements */
@button     : 'tricae';
@divider    : 'tricae';
@flag       : 'tricae';
@header     : 'tricae';
@icon       : 'tricae';
@image      : 'tricae';
@input      : 'tricae';
@label      : 'tricae';
@list       : 'tricae';
@loader     : 'tricae';
@rail       : 'tricae';
@reveal     : 'tricae';
@segment    : 'tricae';
@step       : 'tricae';

/* Collections */
@breadcrumb : 'tricae';
@form       : 'tricae';
@grid       : 'tricae';
@menu       : 'tricae';
@message    : 'tricae';
@table      : 'tricae';

/* Modules */
@accordion  : 'tricae';
@checkbox   : 'tricae';
@dimmer     : 'tricae';
@dropdown   : 'tricae';
@modal      : 'tricae';
@nag        : 'tricae';
@popup      : 'tricae';
@progress   : 'tricae';
@rating     : 'tricae';
@search     : 'tricae';
@shape      : 'tricae';
@sidebar    : 'tricae';
@sticky     : 'tricae';
@tab        : 'tricae';
@transition : 'tricae';
@video      : 'tricae';

/* Views */
@ad         : 'tricae';
@card       : 'tricae';
@comment    : 'tricae';
@feed       : 'tricae';
@item       : 'tricae';
@statistic  : 'tricae';

/* Components */
@box    : 'tricae';
@nav    : 'tricae';
@filter : 'tricae';


/*******************************
         Import Theme
*******************************/

@import "theme.less";

/* End Config */